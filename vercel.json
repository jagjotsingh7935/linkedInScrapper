{
    "builds": [
        {
            "src": "job/wsgi.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "job/wsgi.py"
        }
    ],
    "env": {
        "DJANGO_SETTINGS_MODULE": "job.settings"
    }
}